      }, {
        "type": "color",
        "id": "background-color",
        "label": "fondo del contenedor",
        "default": "#000000"
      }
    ],
    "blocks": [

      {
        "type": "text_block",
        "name": "configuracion",
        "settings": []
      }

    ],
    "presets": [
      {
        "name": "travelheader",
        "category": "Custom content",
        "blocks": []
      }
    ]
  }
{% endschema %}


