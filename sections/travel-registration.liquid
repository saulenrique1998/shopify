
<link rel="stylesheet" href="{{ 'tempusdominus-bootstrap-4.min.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'owl.carousel.min.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'style.css' | asset_url }}" media="print" onload="this.media='all'">
<div class="container-fluid bg-registration py-5" style="margin: 90px 0;  background-image:  url(`{{ section.settings.fondo | img_url:'750x'}}`);background-size: cover;"> 
        <div class="container py-5">
            <div class="row align-items-center">
                <div class="col-lg-7 mb-5 mb-lg-0">
                    <div class="mb-4">
                        <h6 class="text-primary text-uppercase" style="letter-spacing: 5px;">{{ section.settings.title }}</h6>
                        <h1 class="text-white"><span class="text-primary">{{ section.settings.oferta }}</span> </h1>
                    </div>
                    <p class="text-white">{{ section.settings.descripcion }}</p>
                    <ul class="list-inline text-white m-0">
                        <li class="py-2"><i class="fa fa-check text-primary mr-3"></i>Labore eos amet dolor amet diam</li>
                        <li class="py-2"><i class="fa fa-check text-primary mr-3"></i>Etsea et sit dolor amet ipsum</li>
                        <li class="py-2"><i class="fa fa-check text-primary mr-3"></i>Diam dolor diam elitripsum vero.</li>
                    </ul>
                </div>
                <div class="col-lg-5">
                    <div class="card border-0">
                        <div class="card-header bg-primary text-center p-4">
                            <h1 class="text-white m-0">Sign Up Now</h1>
                        </div>
                        <div class="card-body rounded-bottom bg-white p-5">
                            <form>
                                <div class="form-group">
                                    <input type="text" class="form-control p-4" placeholder="Your name" required="required" />
                                </div>
                                <div class="form-group">
                                    <input type="email" class="form-control p-4" placeholder="Your email" required="required" />
                                </div>
                                <div class="form-group">
                                    <select class="custom-select px-4" style="height: 47px;">
                                        <option selected>Select a destination</option>
                                        <option value="1">destination 1</option>
                                        <option value="2">destination 1</option>
                                        <option value="3">destination 1</option>
                                    </select>
                                </div>
                                <div>
                                    <button class="btn btn-primary btn-block py-3" type="submit">Sign Up Now</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<script src="https://code.jquery.com/jquery-3.4.1.min.js" defer="async"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"defer="async"></script>
<script src="{{ 'waypoints.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'counterup.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'easing.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'owl.carousel.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'moment.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'moment-timezone.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'tempusdominus-bootstrap-4.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'main.js' | asset_url }}" defer="defer"></script>

{% schema %}
  {
    "name": "travel-registration",
     "settings": [
       {
          "type": "text",
          "id": "titulo",
          "label": "Titulo de la seccion",
          "default": "Titulo"
        } ,
       {
          "type": "image_picker",
          "id": "fondo",
          "label": "Fondo de la seccion"
          
        } 
     ,
       {
          "type": "text",
          "id": "oferta",
          "label": "Oferta",
          "default": "30% off"
        } 
     ,
       {
          "type": "richtext",
          "id": "descripcion",
          "label": "Descripcion de la oferta",
          "default": "<p>Invidunt lorem justo sanctus clita. Erat lorem labore ea, justo dolor lorem ipsum ut sed eos, ipsum et dolor kasd sit ea justo. Erat justo sed sed diam. Ea et erat ut sed diam sea ipsum est dolor</p>"
        } 
     ],
      "blocks": [
          {
          "type": "text_block",
          "name": "configuracion",
          "settings": [
           
          ]
        }
        
      
        
      ],
      "presets": [
        {
        "name": "travel-registration",
          "category": "Custom content",
          "blocks": [
            
           
            
          ]
        }
        
      ]
  }
{% endschema %}

